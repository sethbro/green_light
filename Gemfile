source 'http://rubygems.org'
gem 'rails', "~> 3.1.0"

# Gems used only for assets and not required
# in production environments by default.
group :assets do
  gem 'sass-rails', "~> 3.1.0"
  gem 'coffee-rails', "~> 3.1.0"
  gem 'uglifier'
end

gem 'jquery-rails'

group :test do
  gem "activerecord", :require => "active_record"
  gem 'rspec'
  gem 'supermodel'
end
